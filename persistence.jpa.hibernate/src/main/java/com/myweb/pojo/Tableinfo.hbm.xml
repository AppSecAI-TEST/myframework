<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.myweb.pojo.Tableinfo" table="tableinfo" schema="web">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="tablename">
            <column name="tablename" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="columnname">
            <column name="columnname" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="chinese">
            <column name="chinese" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="type">
            <column name="type" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="shunxu">
            <column name="shunxu" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>